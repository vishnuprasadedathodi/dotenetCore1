pipeline {
  agent any
  stages {
    stage("Prepare") {
      steps {
        echo "Notify the build status";
        //githubNotify description:"Started build" status: "PENDING"
      }
    }

    stage('Checkout') {
      steps {
        echo "Checking out from Git Repo";
    
       git url: 'https://github.com/vishnuprasadedathodi/dotenetCore1.git',
        branch: 'master'
      }
    } 
    
    stage('Restore packages') {
      steps {
        bat "dotnet restore SampleWeb."
      }
    }



    stage('Build') {
      steps {
        bat "dotnet build   SampleWeb "
        
      }
    }

    stage('Publish') {
      steps {
        bat "dotnet publish  SampleWeb"
      }
    }
    
    stage('Package') {
      steps {
        echo "Deploying to stage environment for more tests!";
        bat "del *.zip"
        
        bat "tar.exe -a -c -f SampleWeb.zip SampleWeb\\bin\\Debug\\netcoreapp3.1\\publish "
       
      }
    }
    
    stage ('Deploy')
    {
    steps{
    
   bat """ 'C:\\Program Files (x86)\\IIS\\Microsoft Web Deploy V3\\msdeploy.exe' -verb:sync -source:IisApp='%WORKSPACE%\\SampleWeb\\bin\\Debug\\netcoreapp3.1\\publish'  -dest:iisapp='test',computerName='127.0.0.1/msdeploy.axd?site=test', authType='basic' , -enableRule:AppOffline """
}
}
    

    
  }
}
